{
    "code-runner.executorMap": {
        "python": "/bin/python3"
    },
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "files.autoSave": "onWindowChange",
    "editor.defaultFormatter": "vscode.html-language-features",
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "php",
        "blade",
        "ejs",
        "jinja",
        "javascript",
        "javascriptreact",
        "plaintext",
        "markdown",
        "vue",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml",
        "HTML (EEx)",
        "HTML (Eex)",
        "plist"
    ],
    "editor.formatOnSave": true,
    "javascript.suggestionActions.enabled": false,
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "explorer.confirmDelete": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.colorTheme": "GitHub Dark",
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.html-language-features",
        "editor.codeActionsOnSave": { /// TSX auto format on save ESLint
            "source.fixAll.eslint": "always"
        }
    },
    "[javascriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.codeActionsOnSave": { /// TSX auto format on save ESLint
            "source.fixAll.eslint": "always"
        }
    },
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "typescript": "typescriptreact"
    },
    "workbench.tree.indent": 10,
    "editor.inlineSuggest.enabled": false,
    "background.enabled": true,
    "background.fullscreen": {
        "images": [
            "https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/fd3407c5-25ad-4b8b-acba-533a753809d8/djai85b-0655be68-f6b0-4691-9ed9-94be6794115b.png/v1/fill/w_1194,h_669,q_70,strp/ocean_s_silent_howl_by_redfoxarts_djai85b-pre.jpg?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.._KItn_4ajN2o0FZkTC7UIsiX3h_ihNXoF2pPNn7--tA"
        ],
        "opacity": 0.1,
        "size": "cover",
        "position": "center",
        "interval": 0,
        "random": false
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.accessibilitySupport": "on",
    "liveshare.accessibility.soundsEnabled": false,
    "accessibility.signals.chatEditModifiedFile": {
